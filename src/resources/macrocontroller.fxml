<ScrollPane fitToHeight="true" fitToWidth="true" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="650.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">

    <FlowPane fx:id="flowpane" xmlns:fx="http://javafx.com/fxml">
        <StepChart fx:id="mvpt" title="MV vs PT" summation="false" prefHeight="300.0" prefWidth="300.0">
            <statistics>
                <FXCollections fx:factory="observableArrayList">
                   <String fx:value="mvpt-M"/>
                   <String fx:value="mvpt-PT"/>
                   <String fx:value="mvpt-V"/>
                </FXCollections>
            </statistics>
        </StepChart>
    </FlowPane>
</ScrollPane>
